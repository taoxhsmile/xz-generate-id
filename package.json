{
  "name": "xz-generate-id",
  "displayName": "Xz Generate Id",
  "description": "",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.53.0"
  },
  "publisher": "taoxhsmile",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:xz-generate-id.generate",
    "onCommand:xz-generate-id.rm-js-nouse"
  ],
  "main": "./out/extension.js",
  "icon": "images/icon.png",
  "contributes": {
    "keybindings": [
      {
        "command": "xz-generate-id.generate",
        "key": "ctrl+1",
        "mac": "cmd+1",
        "when": "editorTextFocus"
      },
      {
        "command": "xz-generate-id.rm-js-nouse",
        "key": "ctrl+2",
        "mac": "cmd+2",
        "when": "resourceLangId == javascript || resourceLangId == javascriptreact || resourceLangId == typescript || resourceLangId == typescriptreact"
      }
    ],
    "commands": [
      {
        "command": "xz-generate-id.generate",
        "title": "生成识别码"
      },
      {
        "command": "xz-generate-id.rm-js-nouse",
        "title": "删除未使用的定义"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "xz-generate-id.generate",
          "group": "navigation"
        },
        {
          "when": "resourceLangId == javascript || resourceLangId == javascriptreact || resourceLangId == typescript || resourceLangId == typescriptreact",
          "command": "xz-generate-id.rm-js-nouse",
          "group": "navigation"
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taoxhsmile/xz-generate-id"
  },
  "scripts": {
    "vscode:publish": "yarn run vscode:prepublish && vsce publish",
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.3",
    "@types/babel__traverse": "^7.14.2",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.53.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "@babel/generator": "^7.15.0",
    "@babel/parser": "^7.15.3",
    "@babel/traverse": "^7.15.0",
    "@babel/types": "^7.15.0"
  }
}
